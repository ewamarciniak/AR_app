<h1>Listing project_team_members</h1>

<table>
  <tr>
    <th>Project</th>
    <th>Team member</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @project_team_members.each do |project_team_member| %>
  <tr>
    <td><%= project_team_member.project %></td>
    <td><%= project_team_member.team_member %></td>
    <td><%= link_to 'Show', project_team_member %></td>
    <td><%= link_to 'Edit', edit_project_team_member_path(project_team_member) %></td>
